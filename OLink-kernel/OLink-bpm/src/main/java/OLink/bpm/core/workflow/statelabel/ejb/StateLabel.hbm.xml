<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="OLink.bpm.core.workflow.statelabel.ejb.StateLabel"
		table="T_STATELABEL">

		<id name="id" column="ID" type="java.lang.String" length="100">
			<generator class="assigned">
				<!--
					To add non XDoclet generator parameters, create a file named
					hibernate-generator-params-StateLabel.xml containing the additional
					parameters and place it in your merge dir.
				-->
			</generator>
		</id>

		<property name="name" type="java.lang.String" update="true"
			insert="true" column="NAME" />

		<property name="orderNo" type="java.lang.String" update="true"
			insert="true" column="ORDERNO" />

		<property name="description" type="java.lang.String" update="true"
			insert="true" column="DESCRIPTION" />

		<property name="value" type="java.lang.String" update="true"
			insert="true" column="VALUE" />

		<property name="sortId" type="java.lang.String" update="true"
			insert="true" column="SORTID" />

		<property name="applicationid" type="java.lang.String"
			update="true" insert="true" column="APPLICATIONID" />

		<!--
			To add non XDoclet property mappings, create a file named
			hibernate-properties-StateLabel.xml containing the additional
			properties and place it in your merge dir.
		-->

	</class>

</hibernate-mapping>
